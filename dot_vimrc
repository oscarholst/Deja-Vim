set nocompatible
filetype on
syntax on
set encoding=utf-8
set tabstop=8
set expandtab
set number
set colorcolumn=80
" let g:airline_theme='base16_chalk'
let g:airline_theme='zenburn'
:set listchars+=space:␣
"  :set listchars+=space:·
autocmd BufRead,BufNewFile *.py let python_highlight_all=1
set splitbelow
set splitright
" Split navigations
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
" Theme stuff
" export TERM=xterm-256color
set t_Co=256
colors zenburn
